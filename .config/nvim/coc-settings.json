{
	"languageserver": {
		"ruby-lsp": {
			"command": "ruby-lsp",
			"initializationOptions": {
				"experimentalFeaturesEnabled": false,
				"enabledFeatures": [
					"codeActions",
					"diagnostics",
					"documentHighlights",
					"documentLink",
					"documentSymbols",
					"foldingRanges",
					"formatting",
					"hover",
					"inlayHint",
					"onTypeFormatting",
					"selectionRanges",
					"semanticHighlighting",
					"completion"
				],
				"formatter": "auto"
			},
			"filetypes": ["ruby"]
		}
	},
	"suggest.autoTrigger": "always",
	"suggest.enablePreview": true,
	"suggest.enablePreselect": true,
	"clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/18.1.3/clangd_18.1.3/bin/clangd",
	"clangd.enabled": true,
	"java.import.gradle.java.home": "/usr/lib/jvm/java-17-openjdk-amd64",
	"sql.database": "postgresql",
	"rpc.showProblems": false,
	"rpc.largeImage": "Working with {LANG}",
	"rpc.smallImage": "Best text editor!",
	"rpc.detailsEditing": "\uD83D\uDCC1 {workspace_folder} {problems}",
	"rpc.lowerDetailsEditing": "\uD83D\uDD27 {file_name}",
	"rpc.detailsViewing": "\uD83D\uDCC2 {workspace_folder} {problems}",
	"rpc.lowerDetailsViewing": " \uD83D\uDC40 {file_name}",
	"rpc.checkIdle": true,
	"rpc.idleText": "Ngopi",
	"rpc.idleTimeout": 300,
	"rpc.detailsIdling": "\u2615",
	"rpc.lowerDetailsIdling": "entah ngopi atau ketiduran",
	"rpc.lowerDetailsNotFound": "{empty}",
	"coc.source.file.enable": true,
	"coc.preferences.bracketEnterImprove": true,
	"prettier.tabWidth": 2,
	"prettier.useTabs": true,
	"prettier.disableLanguages": ["c", "cpp"],
	"emmet.includeLanguages": { "htmldjango": "html" },
	"python.formatting.provider": "black",
	"python.formatting.blackPath": "black",
	"python.linting.enabled": true,
	"pyright.completion.importSupport": true,
	"pyright.completion.snippetSupport": true,
	"htmldjango.enable": true,
	"htmldjango.djhtml.tabWidth": 2,
	"htmldjango.formatting.provider": "djhtml",
	"volar.autoClosingTags": true,
	"coc.preferences.formatOnSave": true,
	"solargraph.autoformat": true,
	"solargraph.formatting": true,
	"solargraph.symbols": true,
	"html-css-support.styleSheets": [
		"https://cdn.tailwindcss.com",
		"style.css",
		"/style.css"
	],
  "html-css-support.enabledLanguages": [
    "html",
    "vue"
  ],
  "snippets.ultisnips.pythonPrompt": false
}
